#include <stdio.h>
int main(){
	char first[100];
	char second[100];
	printf("请输入两个字符串\n");
	gets(first);
	gets(second);
	int i,j;
	for(i=0;first[i]!='\0'; i++);
	for(j=0;second[j]!='\0';j++)
	{
		first[i+j]=second[j];
		
	}
	first[i+j]='\0';
	printf("%s\n",first);
	return 0;
}
#include <stdio.h>
#include <string.h>
int main(){
	char first[100];
	char second[100];
	printf("请输入两个字符串\n");
	gets(first);
	gets(second);
	strcat(first,second);
	puts(first);
	return 0;
}
